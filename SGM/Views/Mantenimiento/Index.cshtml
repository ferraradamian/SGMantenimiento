@model IEnumerable<SGM.Models.Mantenimeinto>

@{
    ViewBag.Title = "Mantenimiento";
}

<h2>Mantenimiento</h2>

<p>
    @Html.ActionLink("Crear nuevo Mantenimiento", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fecha)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tipo_matenimiento)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.repetir_cada)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.nombre)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.descripcion)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.fecha)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.tipo_matenimiento)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.repetir_cada)
    </td>
    <td>
        @Html.ActionLink("Editar", "Edit", new { id = item.mantenimeinto_id }) |
        @Html.ActionLink("Detalle", "Details", new { id = item.mantenimeinto_id }) |
        @Html.ActionLink("Eliminar", "Delete", new { id = item.mantenimeinto_id })
    </td>
</tr>
}

</table>
