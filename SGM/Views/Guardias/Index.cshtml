@model IEnumerable<SGM.Models.Guardia>

@{
    ViewBag.Title = "Guardia";
}

<h2>Guardia</h2>

<p>
    @Html.ActionLink("Crear Nueva Guardia", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.coodinador_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fecha)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.horaInicio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.horaFin)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.coodinador_id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.fecha)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.horaInicio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.horaFin)
            </td>
            <td>
                @Html.ActionLink("Editar", "Edit", new { id = item.guardia_id }) |
                @Html.ActionLink("Detalle", "Details", new { id = item.guardia_id }) |
                @Html.ActionLink("Eliminar", "Delete", new { id = item.guardia_id })
            </td>
        </tr>
    }
    </table>
